cat > .gitignore << 'EOF'
# Dependencies
node_modules/
*/node_modules/

# Environment files  
.env
*/.env

# Build files
build/
dist/
*/build/
*/dist/

# Uploads
uploads/
*/uploads/

# Logs
*.log
npm-debug.log*

# OS files
.DS_Store
Thumbs.db

# IDE
.vscode/
.idea/

# N8N configurations with tokens
*n8n*.json

# Backup files
*.backup

# Temporary files
*.tmp
*.temp
EOF